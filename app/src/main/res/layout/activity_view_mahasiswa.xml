<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    card_view:cardCornerRadius="6dp"
    card_view:cardElevation="3dp"
    card_view:cardUseCompatPadding="true">
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".viewMahasiswa"
    android:orientation="vertical"
    >


    <!-- TextView Nama Mahasiwa -->
    <TextView
        android:id="@+id/txt_nama_mahasiswa"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <!-- TextView Nama Panggilan Mahasiswa -->
    <TextView
        android:id="@+id/txt_namaPanggilan_mahasiswa"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <!-- TextView NoHp Mahasiswa -->
    <TextView
        android:id="@+id/txt_nohp_mahasiswa"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
    <!-- TextView NoHp Mahasiswa -->
    <TextView
        android:id="@+id/txtemail"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />



</LinearLayout>
</androidx.cardview.widget.CardView>